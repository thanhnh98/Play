<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">

            <padding
                android:left="2dp"
                android:bottom="2dp"
                android:top="2dp"
                android:right="2dp"/>
            <size
                android:width="5dp"
                android:height="5dp"/>
            <stroke
                android:width="1dp"
                android:color="#FF6F00"/>
            <gradient
                android:startColor="#FFF8E1"
                android:endColor="#FFB74D"/>
        </shape>
    </item>
</layer-list>